<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:pixlui="http://schemas.android.com/apk/com.neopixl.pixlui"
  	xmlns:tools="http://schemas.android.com/tools"
    package="com.example.humbleanalytics"
    android:versionCode="3"
    android:versionName="3.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" 
        android:name="com.example.humbleanalytics.AnalyticsApplication"
        >
        
        
        <activity
            android:name="com.example.humbleanalytics.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.humbleanalytics.DisplayAppData"
            android:label="@string/title_activity_display_app_data"
            android:parentActivityName="com.example.humbleanalytics.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.humbleanalytics.MainActivity" />
        </activity>
    </application>

</manifest>
